{
  "name": "figma-plugin-really-flatten-vectors",
  "version": "1.0.0",
  "description": "Flattens vectors and merges their path networks",
  "author": "Steven La <mrstevenla@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.44.0",
    "parcel": "^2.5.0",
    "typescript": "*"
  },
  "scripts": {
    "build": "./node_modules/.bin/parcel build",
    "watch": "./node_modules/.bin/parcel watch"
  },
  "targets": {
    "plugin": {
      "source": "code.ts",
      "includeNodeModules": true,
      "context": "browser",
      "outputFormat": "global",
      "engines": {
        "browsers": "Chrome 40"
      }
    }
  }
}
